<h1>Welcome to Multi Variate Network Explorer</h1>



<section class="page-section bg-dark clearfix">
    <form method="post" asp-action="FileUpload" asp-controller="Home" enctype="multipart/form-data">
        <div class="form-group">
            <input class="inputfile" type="file" id="file" name="file" data-caption="{filePath}">
            <label for="file"><strong>Choose a file</strong></label>
        </div>
        <input type="submit" class="btn btn-primary" value="Upload" />
        <p style="color:red;">
            @ViewBag.Messsage
        </p>
    </form>
</section>



